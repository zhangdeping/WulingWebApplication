@using WulingWebApplication.Models
@using Webdiyer.WebControls.Mvc
@model PagedList<PassengerVehicle>
<table class="table table-bordered table-striped">
    <tr>
        <th class="nowrap">序号</th>
        <th>
            @Html.DisplayNameFor(model => model.时间)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.省)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.市)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.县)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.品牌)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.制造商)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.发动机型号)
        </th>
        
    </tr>
    @{ int i = 0;}
    @foreach (var item in Model)
    {
        <tr>
            <td>@(Model.StartItemIndex + i++)</td>
            <td>
                @Html.DisplayFor(modelItem => item.时间)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.省)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.市)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.县)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.品牌)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.制造商)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.发动机型号)
            </td>
        </tr>
    }
</table>
